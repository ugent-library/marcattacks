(
    $type := $marcmap0("920a");

    { 
        "@id": "my:" & $marcmap0("001"),
        "@type": $type = "book" ? "schema:Book" : "schema:CreativeWork",
        "name": $marcmap("245ab") ~> $join(" ") ~> $strip() ,
        "subject": $marcmap("500a") ~> $map(function ($val) {
            { 
                "@id": $genid(),
                "@type": "schema:Subject",
                "name": $val 
            }
        })
    }
)